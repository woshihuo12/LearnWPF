<Window x:Class="mumu_tabandMnemonics.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
    Title="Tab键和快捷键" Height="300" Width="300">
    <StackPanel>
        <Button Click="cmdClicked" Margin="5">Use Alt+_A</Button>
        <WindowsFormsHost Grid.Row="1" Margin="5">
            <wf:Button Text="Use Alt+&amp;B" Click="cmdClicked"></wf:Button>
        </WindowsFormsHost>
        <Button Grid.Row="2" Click="cmdClicked" Margin="5">Use Alt+_C</Button>
    </StackPanel>
</Window>
